{
  "xsappname": "bookshopcap",
  "tenant-mode": "dedicated",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "$XSAPPNAME.Viewer",
      "description": "Viewer"
    },
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Administrator"
    }
  ],
  "role-templates": [
    {
      "name": "Viewer",
      "description": "Viewer",
      "scope-references": [
        "$XSAPPNAME.Viewer"
      ]
    },
    {
      "name": "Admin",
      "description": "Administrator",
      "scope-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ],
  "oauth2-configuration": {
    "redirect-uris": [
      "http://localhost:4004/**",
      "https://*.cfapps.us10-001.hana.ondemand.com/**",
      "https://*.trial.applicationstudio.cloud.sap/**",
      "https://*.cf.us10-001.hana.ondemand.com/**" 
      ]
  },
  "attributes": []
}
